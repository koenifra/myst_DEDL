ea_def.to_cartopy_crs()\nfig, ax = plt.subplots(subplot_kw=dict(projection=crs))\ncoastlines = ax.coastlines()  \nax.set_global()\n\n#plot\nimg = plt.imshow(result, transform=crs, extent=crs.bounds, origin='upper')\n# Calculate (height_of_image / width_of_image)\nim_ratio = result.shape[0]/result.shape[1]\n \n# Plot vertical colorbar\nplt.colorbar(fraction=0.047*im_ratio)\nplt.show()","visibility":"show","key":"IVZR5isVfd"},{"type":"output","id":"gb0d0Ria_VQyMzCBmG84I","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"ef0abf1e0655b71a8ddf11bf3873b72a","path":"/ef0abf1e0655b71a8ddf11bf3873b72a.png"},"text/plain":{"content":"<Figure size 1500x1500 with 2 Axes>","content_type":"text/plain"}}}],"visibility":"show","key":"dKhuLIdncO"}],"visibility":"show","key":"EKUHLnO1Nv"}],"key":"hPUSt6DQs6"},"references":{"cite":{"order":[],"data":{}}}}